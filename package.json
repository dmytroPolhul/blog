{
  "name": "blog-graphql",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "generate": "drizzle-kit generate:pg",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "mc": "typeorm migration:create ./src/database/migrations/${NAME}",
    "mg": "npm run typeorm -- --d src/config/orm.config.ts migration:generate ./src/database/migrations/${NAME}",
    "mr": "npm run typeorm -- --d src/config/orm.config.ts migration:run",
    "mre": "npm run typeorm -- --d src/config/orm.config.ts migration:revert",
    "pre-commit": "npm run lint && npm run format",
    "prepare": "husky install",
    "commit": "cz"
  },
  "dependencies": {
    "@apollo/server": "4.9.4",
    "@as-integrations/fastify": "^2.1.1",
    "@magidoc/cli": "^4.0.0",
    "@nestjs/apollo": "^12.0.9",
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/graphql": "^12.0.9",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-fastify": "^10.3.0",
    "@nestjs/typeorm": "^10.0.0",
    "@nestled/config": "^2.3.8",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "commitizen": "^4.3.0",
    "fastify-cookie": "^5.7.0",
    "fastify-passport": "^0.6.0",
    "drizzle-orm": "^0.29.3",
    "graphql": "^16.8.1",
    "graphql-tools": "^9.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "cz-conventional-changelog": "^3.3.0",
    "drizzle-kit": "^0.20.10",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "main.ts",
      ".*.module.ts$",
      "database/.*",
      "config/.*",
      "modules/baseModule/.*"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
